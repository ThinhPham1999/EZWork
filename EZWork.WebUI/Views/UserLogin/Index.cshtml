
@model IEnumerable<EZWork.Core.Entities.EZUser>

<div class="container ">
    <div class="row">
        <div class="label-title col-12 ">
            <div class=" form-group col-sm-2">
                @Html.DisplayNameFor(model => model.UserName)
            </div>
            <div class="col-sm-1">:</div>
            <div class="form-group col-sm-6 ">
                @foreach (var item in Model)
                {
                    @Html.DisplayFor(modelItem => item.UserName)
                }
            </div>
        </div>
        <div class="label-title col-12 ">
            <div class="form-group col-sm-2">
                @Html.DisplayNameFor(model => model.FullName)
            </div>
            <div class="col-sm-1">:</div>
            <div class="form-group col-sm-6">
                @foreach (var item in Model)
                {
                    @Html.DisplayFor(modelItem => item.FullName)
                }
            </div>
        </div>
        <div class="label-title col-12 ">
            <div class="form-group col-sm-2">
                @Html.DisplayNameFor(model => model.Gender)
            </div>
            <div class="col-sm-1">:</div>
            <div class="form-group col-sm-6">
                @foreach (var item in Model)
                {
                    @Html.DisplayFor(modelItem => item.Gender)
                }
            </div>
        </div>
        <div class="label-title col-12 ">
            <div class="form-group col-sm-2">
                @Html.DisplayNameFor(model => model.BirthDay)
            </div>
            <div class="col-sm-1">:</div>
            <div class="form-group col-sm-6">
                @foreach (var item in Model)
                {
                    @Html.DisplayFor(modelItem => item.BirthDay)
                }
            </div>
        </div>
        <div class="label-title col-12 ">
            <div class="form-group col-sm-2">
                @Html.DisplayNameFor(model => model.EZAccount.Email)
            </div>
            <div class="col-sm-1">:</div>
            <div class="form-group col-sm-6">
                @foreach (var item in Model)
                {
                    @Html.DisplayFor(modelItem => item.EZAccount.Email)
                }
            </div>
        </div>
        <div class="label-title col-12 ">
            <div class="form-group col-sm-2">
                @Html.DisplayNameFor(model => model.EZAccount.PhoneNumber)
            </div>
            <div class="col-sm-1">:</div>
            <div class="form-group col-sm-6">
                @foreach (var item in Model)
                {
                    @Html.DisplayFor(modelItem => item.EZAccount.PhoneNumber)
                }
            </div>
        </div>
        <div class="label-title col-12 ">
            <div class="form-group col-sm-2">
                @Html.DisplayNameFor(model => model.Status)
            </div>
            <div class="col-sm-1">:</div>
            <div class="form-group col-sm-6">
                @foreach (var item in Model)
                {
                    @Html.DisplayFor(modelItem => item.Status)
                }
            </div>
        </div>
        <div class="label-title col-12 ">
            <div class="form-group col-sm-2">
                @Html.DisplayNameFor(model => model.ImageProfile)
            </div>
            <div class="col-sm-1">:</div>
            <div class="form-group col-sm-6">
                @foreach (var item in Model)
                {
                    @Html.DisplayFor(modelItem => item.ImageProfile)
                }
            </div>
        </div>
        <div class="label-title col-12 ">
            <div class="form-group col-sm-2">
                @Html.DisplayNameFor(model => model.CreateAt)
            </div>
            <div class="col-sm-1">:</div>
            <div class="form-group col-sm-6">
                @foreach (var item in Model)
                {
                    @Html.DisplayFor(modelItem => item.CreateAt)
                }
            </div>
        </div>
        <div class="label-title col-12 ">
            <div class="form-group col-sm-2">
                @Html.DisplayNameFor(model => model.ModifierAt)
            </div>
            <div class="col-sm-1">:</div>
            <div class="form-group col-sm-6">
                @foreach (var item in Model)
                {
                    @Html.DisplayFor(modelItem => item.ModifierAt)
                }
            </div>
        </div>
        @foreach (var item in Model)
        {
            <button class="btn btn-outline-secondary">
                @Html.ActionLink("Edit", "UpdateProfileUser", new { id = item.Id })
            </button>
        }

        <table class="table ">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.FullName)
                </th>

                <th>
                    @Html.DisplayNameFor(model => model.Gender)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.BirthDay)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EZAccount.Email)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EZAccount.PhoneNumber)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ImageProfile)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CreateAt)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ModifierAt)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Status)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.UserName)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>

                    <td>
                        @Html.DisplayFor(modelItem => item.FullName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Gender)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.BirthDay)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.EZAccount.Email)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.EZAccount.PhoneNumber)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ImageProfile)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CreateAt)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ModifierAt)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Status)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.UserName)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "UpdateProfileUser", new { id = item.Id })
                    </td>
                </tr>
            }

        </table>
    </div>
</div>