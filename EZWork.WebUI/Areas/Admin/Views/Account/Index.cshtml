@model EZWork.WebUI.Areas.Admin.ViewModels.ListAccountViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}
<div class="row">
    <div class="col col-lg-12">
        <form>
            <div class="input-group">
                <input type="text" class="form-control inputSeach" placeholder="Search" name="searchTerm" />
                <div class="input-group-append">
                    <button class="btn btn-outline-primary" type="submit"><i class="fas fa-search mr-1"></i>Search</button>
                    <button class="btn btn-outline-secondary createBtn" type="button" data-href="@Url.Action("Action","Category")" data-toggle="modal" data-target="#exampleModal"><i class="fas fa-plus mr-1"></i>Create</button>
                    <button class="btn btn-outline-success resetBtn" data-href="@Url.Action("Index","Category")"><i class="fas fa-redo-alt mr-1"></i>Reset</button>
                </div>
            </div>
        </form>
    </div>
</div>
<div class="row">
    <div class="col col-lg-12">
        @{ Html.RenderPartial("Listing", Model);}
    </div>
</div>
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg">
    </div>
</div>

